{
  "name": "adeebali.com",
  "version": "1.0.0",
  "description": "Code for my website.",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "xo && nyc jest"
  },
  "keywords": [
    "Adeeb Ali",
    "website",
    "nextjs",
    "nodejs",
    "react",
    "xo",
    "jest"
  ],
  "author": "Adeeb Ali",
  "license": "",
  "dependencies": {
    "@chakra-ui/icons": "2.0.19",
    "@chakra-ui/react": "2.6.0",
    "@chakra-ui/theme-tools": "2.0.16",
    "@emotion/react": "11.10.8",
    "@emotion/styled": "11.10.8",
    "framer-motion": "^5.5.5",
    "next": "13.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.8.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.3",
    "eslint": "8.39.0",
    "eslint-config-next": "13.2.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.5.0",
    "nyc": "15.1.0",
    "xo": "0.54.2"
  },
  "xo": {
    "envs": [
      "node",
      "jest"
    ],
    "extends": [
      "next"
    ],
    "rules": {
      "node/file-extension-in-import": 0,
      "import/no-unassigned-import": 0,
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0,
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    },
    "parser": "@babel/eslint-parser"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}