{
  "name": "adeebali.com",
  "version": "1.0.0",
  "description": "Code for my website.",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "xo && nyc jest"
  },
  "keywords": [
    "Adeeb Ali",
    "website",
    "nextjs",
    "nodejs",
    "react",
    "xo",
    "jest"
  ],
  "author": "Adeeb Ali",
  "license": "",
  "dependencies": {
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.3",
    "@chakra-ui/theme-tools": "^1.3.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "framer-motion": "^5.5.5",
    "next": "^12.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "eslint": "^8.5.0",
    "eslint-config-next": "^12.0.7",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^27.5.1",
    "nyc": "^15.1.0",
    "xo": "^0.47.0"
  },
  "xo": {
    "envs": [
      "node",
      "jest"
    ],
    "extends": [
      "next"
    ],
    "rules": {
      "node/file-extension-in-import": 0,
      "import/no-unassigned-import": 0,
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0,
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    },
    "parser": "@babel/eslint-parser"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}