{
  "name": "adeebali.com",
  "version": "1.0.0",
  "description": "Code for my website.",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "xo && nyc ava"
  },
  "keywords": [
    "Adeeb Ali",
    "website",
    "nextjs",
    "nodejs",
    "react",
    "xo",
    "ava"
  ],
  "author": "Adeeb Ali",
  "license": "",
  "dependencies": {
    "next": "^9.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "ava": "^3.7.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-unicorn": "^18.0.1",
    "nyc": "^15.0.1",
    "xo": "^0.29.1"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "extends": [
      "xo-react"
    ],
    "rules": {
      "import/no-unassigned-import": 0,
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0,
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    },
    "plugins": [
      "unicorn"
    ],
    "parser": "babel-eslint"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*",
      "pages/**/*"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}